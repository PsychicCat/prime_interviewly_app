md-dialog.interviewer(flex="40")
   md-toolbar
      h2.md-display-1(layout="row" layout-margin="auto") Add Interviewer
   form(name="interviewerForm" layout="row")
   md-dialog-content()
      .inputs(layout="column")
               md-input-container
                  label Name
                  input(type="text")
               md-input-container
                  label Title
                  input(type="text")
               md-input-container
                  label Company
                  input(type="text")
               md-input-container
                  label Company Link
                  input(type="text")
               md-input-container
                  label Company Description
                  input(type="text")

            .loginbut(layout="row" layout-align="center center")
               md-button.md-raised() Save
               md-button.md-raised.md-warn(ng-click="close()") Cancel
