md-dialog(flex="40" style="max-height:700px")
   md-toolbar
      p Add Interviewer
   form
      md-content
            .inputs
               md-input-container
                  label Name
                  input(type="text")
               md-input-container
                  label Title
                  input(type="text")
               md-input-container
                  label Company
                  input(type="text")
               md-input-container
                  label Link
                  input(type="text")
               md-input-container
                  label Description
                  input(type="text")
               md-input-container
                  label Select Event
                  md-select(ng-model="event")
                     md-option(ng-repeat="event in events")
                        p {{event.name}}
               md-input-container
                  label Start Time
                  input(type="time")
               md-input-container
                  label End Time
                  input(type="time")
               md-input-container
                  label Select Cohort
                  md-select(ng-model="cohort")
                     md-option(ng-repeat="cohort in cohorts")
                        p {{cohort.name}}
            md-button Save
            md-button Cancel
