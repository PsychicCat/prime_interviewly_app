.student
    md-toolbar
        p Add Students
    md-content(flex="33")
        form(name="studentForm")
            md-input-container
                label Student Name
                input(ng-model="student.name" type="text")
            md-input-container
                label Cohort
                input(ng-model="student.cohort" type="text")
            md-input-container
                label Select Event
                md-select(ng-model="event")
                    md-option(ng-repeat="event in events")
                        p {{event.name}}
            md-input-container
                label Rankings
                isteven-multi-select(
                input-model="companies"
                output-model="selections"
                button-label="name"
                item-label="name"
                tick-property="ticked")
            md-button Save
            md-button Cancel